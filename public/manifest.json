{
  "short_name": "My Wallet",
  "name": "Swaptacular Wallet",
  "description": "Allows you to connect to a creditors agent and make and request payments in Swaptacular digital currencies.",
  "lang": "en",
  "dir": "ltr",
  "start_url": "./",
  "scope": "./",
  "display": "standalone",
  "orientation": "portrait",
  "theme_color": "#ffffff",
  "background_color": "#ffffff",
  "icons": [
    {
      "src": "icons/favicon-192x192.png",
      "type": "image/png",
      "sizes": "192x192"
    },
    {
      "src": "icons/favicon-512x512.png",
      "type": "image/png",
      "sizes": "512x512"
    }    
  ],
  "file_handlers": [
    {
      "action": "./open-pr0",
      "name": "PR-zero Payment Request Document",
      "accept": {
        "application/vnd.swaptacular.pr0": [ ".pr0" ]
      },
      "icons": [
        {
          "src": "icons/favicon-192x192.png",
          "type": "image/png",
          "sizes": "192x192"
        },
        {
          "src": "icons/favicon-512x512.png",
          "type": "image/png",
          "sizes": "512x512"
        }
      ]
    }
  ]
}
